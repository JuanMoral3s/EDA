    Desv Estandar 
*/
#include <stdio.h>
#include <math.h>

double sigma();

int main()
{
    printf("%f", sigma());
    
    return 0;
    
}

double sigma(){
    
    int n = 5;
    float a1[n];
    float a2[n];
    for ( int i = 0 ; i < n ; ++i)
    {
    a1[i] = 4.5;
    a2[i] = 5;
    }
    
    float a = n - 1 ;
    printf("%f\n", a);
    float b = 1 / a;
    printf("%f\n", b);
    float c = 0;
    
    for (int i = 0 ; i < n ; ++i)
    {
    c = c + (pow((a1[i] - a2[i]), 2));
    printf("%f\n", c);
    }
    float e = c * b;
    printf("%f\n", e); 
    float f = sqrt(e);
    
    return f; 
    
}
    
